<% provide(:title, @user.name) %>
<head>
  <link rel='stylesheet' href='https://cdnjs.cloudflare.com/ajax/libs/fullcalendar/3.7.0/fullcalendar.min.css' />
</head>

<div class="row">
  <aside class="col-md-4">
    <section class="user_info">
      <h1>
        <%= gravatar_for @user %>
        <%= @user.name %>
      </h1>
    </section>
  </aside>
</div>
<div>  <%= link_to "Create event", '/create_event', class: "btn btn-lg btn-primary" %></div>
<div id="calendar" data-eventsfiltered="<%= @eventsfiltered %>"></div>